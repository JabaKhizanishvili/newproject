<?xml version="1.0" encoding="UTF-8"?>
  <!DOCTYPE install SYSTEM "http://web.magti.ge/pda/defines/xml/grid.dtd">
<install type="component" version="1.5.0">
	<filters class="page_filter_block" type="h">
		<filter name="firstname" type="stext" default="" label="firstname" class="kbd" />
		<filter name="lastname" type="stext" default="" label="lastname" class="kbd" />
		<filter name="private_number" type="text" default="" label="private_number" />
		<filter name="position" type="stext" default="" label="position" class="kbd" />
		<filter name="tablenum" type="text" default="" label="TABLENUM" />
		<filter name="org" type="org" default="" label="org" />
		<filter name="unit" type="unit" active="1" default="" label="unit"  elementid="org" />
		<filter name="staffschedule" type="staffschedule" label="staffschedule"  default="-1" elementid="org" />
		<filter name="chief_schedule" type="stext" default="" label="chief_schedule" />
		<filter name="userrole" type="userrole" default="" label="User_Role" />
		<filter name="category" type="sql" default="" label="CATEGORY_ID" key_field="ID" value_field="LIB_TITLE" query="select id, lib_title from lib_categories t where t.active=1 order by t. lib_title asc" select_label="Select Category" must="" />
		<filter name="regime" type="state" default="-1" label="CALCULUS_REGIME">
			<option value="-1">select category</option>
			<option value="1">Office</option>
			<option value="2">Distance</option>
		</filter>
		<filter name="calculus_type" type="state" default="-1" label="CALCULUS_TYPE">
			<option value="-1">Select CALCULUS_TYPE</option>
			<option value="1">Digital</option>
			<option value="2">Material</option>
			<option value="3">Excluded</option>
		</filter>
		<filter name="attributes" type="attributes" destinations="2,3" default="0" label="Attributes" />
		<filter name="permit_id" type="text" default="" label="permit_id" />
		<filter name="graphtype" type="graphtype" default="-1" label="graphtype" />
		<!--<filter name="graphgroup" type="graphgroup" default="" label="graphgroup" />-->
		<filter name="chief_firstname" type="stext" default="" label="Chief Firstname" />
		<filter name="chief_lastname" type="stext" default="" label="Chief Lastname" />
		<filter name="active" type="state" default="1" label="State">
			<option value="-1">Select State</option>
			<option value="1">current contract</option>
			<option value="0">paused contract</option>
			<option value="-2">Canceled Contract</option>
			<option value="-3">schedulechanging</option>
			<option value="-6">Rollbacked</option>
		</filter>
		<filter type="buttons" default="" submit_label="Filter" reset_label="Clear" show_reset= "1" />
	</filters>
	<params class="table table-bordered table-condensed table-hover table-striped" >
		<param order="" type="number" default="" label="#" rclass="text-center" />
		<param order="" type="checkall" default="" label="" rclass="text-center"   />
		<param order="pr.firstname" type="editlink" label="FIRSTNAME" key="FIRSTNAME" rclass="text-left" T="1" Tscope="person" />
		<param order="pr.lastname" type="editlink" label="LASTNAME" key="LASTNAME" rclass="text-left" T="1" Tscope="person" />
		<param order="t.org" type="org" label="ORG" key="ORG" rclass="text-left"  T="1"/>
		<param order="" type="mainunit" label="MAINUNIT" key="ORG_PLACE" tipkey="MAINUNIT" rclass="text-left"  T="1"/>
		<param order="un.lib_title" type="print" label="STRUCTURE" key="ORG_PLACE_NAME" rclass="text-left"  T="1"/>
		<param order="t.staff_schedule" type="staffschedule" label="staffschedule" key="STAFF_SCHEDULE" rclass="text-left" T="1"/>
		<param order="lss.position" type="position" label="position" key="POSITION" rclass="text-left" T="1"/>
		<param order="" type="chiefstooltip" label="chiefs" length="2" word-separator=", " rclass="text-left" T="1"/>
		<param order="lss.chief_schedule" type="staffschedule" label="CHIEF_SCHEDULE" rclass="text-center" key="CHIEF_SCHEDULE" T="1"/>
		<param order="t.contract_type" type="switch" label="CONTRACT_TYPE" key="CONTRACT_TYPE" rclass="text-left">
			<option value="1">Standard Contract</option>
		</param>
		<param order="" type="attributes" label="Attributes" length="2" word-separator=", " item-type="2" rclass="text-left" />
		<param order="t.contracts_date" type="date" format="%Y-%m-%d" label="CONTRACTS_DATE" key="CONTRACTS_DATE" rclass="text-left"  />
		<param order="t.contract_end_date"  type="date" format="%Y-%m-%d" label="CONTRACT_END_DATE" key="CONTRACT_END_DATE" rclass="text-left"  />
		<!--		<param order="pr.change_date"  type="date" format="%Y-%m-%d" label="ASSIGNMENT_DATE" key="CHANGE_DATE" rclass="text-left"  />-->
		<param order="assignment_date" type="date" format="%Y-%m-%d" label="ASSIGNMENT_DATE" rclass="text-center" key="ASSIGNMENT_DATE" />
		<param order="t.active" type="wstatus" label="STATUS" rclass="text-center" key="ACTIVE" />
		<param order="t.release_files" type="fileprint" label="RELEASE_FILES" key="RELEASE_FILES" rclass="text-center" />
		<param order="" type="print" label="changing" rclass="text-center" />
		<param order="t.id" type="print" label="ID" key="ID" rclass="text-center" />
	</params>
</install>
