<?xml version="1.0" encoding="UTF-8"?>
<install type="component" version="1.5.0">
	<params>
		<param name="LIB_TITLE" type="text" default="" label="Title" must="1"  geokbd="1" />
		<param name="LIB_DESC" type="textarea" default="" label="Description" geokbd="1" />
		<param name="ACTIVE" type="radio" default="1">
			<option value="1">Active Status</option>
			<option value="0">Inactive</option>
		</param>
	</params>
	<params group="msg_editors">
		<param name="MSG_TYPE" type="radio" default="0" label="MSG_TYPE">
			<option value="0">SMS_EMAIL_OPTION</option>
			<option value="1">SMS_OPTION</option>
			<option value="2">EMAIL_OPTION</option>
		</param>
		<param name="SMS" type="tmplarea" default="" label="SMS_OPTION_TXT" geokbd="1" rows="5"  />
		<param name="LNG_GEO" type="radio" default="0"  label="LNG_GEO">
			<option value="1">YES</option>
			<option value="0">NO</option>
		</param>
		<param name="EMAIL_TITLE" type="tmpltext" default="" label="EMAIL_OPTION_LBL" geokbd="1"/>
		<param name="EMAIL" type="tmpleditor" default="" label="EMAIL_OPTION_TXT" rows="20" geokbd="1" />
		<param name="ID" type="hidden" default="" />
	</params>
</install>
