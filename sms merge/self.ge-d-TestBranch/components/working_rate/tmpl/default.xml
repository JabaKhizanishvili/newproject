<?xml version="1.0" encoding="UTF-8"?>
<install type="component" version="1.5.0">
	<params>
		<param name="LIB_TITLE" type="text" default="" label="Title" must="1"  geokbd="1" />
		<param name="LIB_DESC" type="textarea" default="" label="Description" geokbd="1" />
		<group>
			<param name="ACTIVE" type="radio" default="1">
				<option value="1">Active Status</option>
				<option value="0">Inactive</option>
			</param>
			<param name="ORDERING" type="text" default="999" label="ORDERING"  />
		</group>
	</params>
	<params group="details">
		<param name="WORK_DURATION" type="number" default="" must="1" label="WORK_DURATION" />
		<param name="WORK_TYPE" type="radio" default="1" label="WORK_TYPE" must="1" >
			<option value="1">Full Time</option>
			<option value="2">Part Time</option>
		</param>
		<param name="ID" type="hidden" default="" />
	</params>
</install>
