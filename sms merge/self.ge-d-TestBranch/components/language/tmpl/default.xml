<?xml version="1.0" encoding="UTF-8"?>
<install type="component" version="1.5.0">
	<params>
		<param name="LIB_CODE" type="text" default="" label="lib_code" must="1"/>
		<param name="LIB_TITLE" type="text" default="" label="lib_title" must="1"/>
	</params>
	<params group="add">
		<param name="LIB_DESC" type="text" default="" label="LIB_DESC" />
		<param name="LIB_LEVEL" type="hidden" default="" must="1" />
		<param name="ACTIVE" type="radio" default="1"  must="1">
			<option value="1">Active Status</option>
			<option value="0">Inactive</option>
		</param>
		<param name="CAPITAL" type="radio" default="0"  must="1">
			<option value="1">Yes</option>
			<option value="0">No</option>
		</param>
		<param name="ID" type="hidden" default="" />
	</params>
</install>
