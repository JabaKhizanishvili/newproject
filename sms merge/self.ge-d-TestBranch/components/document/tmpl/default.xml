        <param name="LIB_DESC" type="text" default="" label="Description"/>
    </params>
    <params group="details">
        <param name="FILES" type="files" default="" label="FILES" must="1"/>
        <param name="MUST" type="radio" label="Musttitle" default="" must="1">
            <option value="1">Must</option>
            <option value="0">Notmust</option>
        </param>
        <param name="WORKER" type="slfstaff" case="1" default="" label="Workers" must="1"/>
