<?xml version="1.0" encoding="UTF-8"?>
<install type="component" version="1.5.0">
	<params>
		<param name="ORG" type="orgprint" default="" label="ORG" key="ID" />
		<param name="WORKERS"  type="slfstaff" case="2" label="PERSON" must="1"/>
		<param name="CHANGE_SUB_TYPE" type="changetypes" default="" label="CHANGE_TYPE"  />
		<param name="SALARYTYPE" type="salarytypes" default="-1" label="SALARYTYPE" />
		<param name="SALARY" type="number" default="" label="SALARY" />
		<param name="IBAN" type="text" default="" label="IBAN" />
		<param name="SALARY_PAYMENT_TYPE" default="2" type="radio" >
			<option value="1">Cash</option>
			<option value="2">Card</option>
		</param>
		<param name="CHIEFS" type="slfstaff" case="1" default="" label="ADDITIONAL_CHIEFS" />
	</params>
	<params group="more">
		<param name="TABLENUM" type="text" default="" label="TABLENUM"  mask=""  />
		<param name="P_CODE" type="text" default="" label="P_CODE"  mask=""  />
		<param name="CONTRACTS_DATE" type="calendar"  default="" label="CONTRACTS_DATE"  />
		<param name="CONTRACT_END_DATE" type="calendar" default="" label="CONTRACT_END_DATE"  />
		<param name="ACCOUNTING_OFFICES" type="officeschosen" label="ACCOUNTING_OFFICE"  />
		<param name="ATTRIBUTES" type="attributechosen" label="ATTRIBUTES" destinations="2,3" />
		<!--<param name="TEST_DATE" type="calendar" default="" label="TEST_DATE" must="" />-->
		<param name="AUTO_OVERTIME" type="radio" default="0"  >
			<option value="1">On</option>
			<option value="0">Off</option>
		</param>
		<param name="CALCULUS_TYPE" type="list" default="-1" label="CALCULUS_TYPE"  >
			<option value="-1">Select CALCULUS_TYPE</option>
			<option value="1">Digital</option>
			<option value="2">Material</option>
			<option value="3">Excluded</option>
		</param>
		<param name="CALCULUS_REGIME" type="radio" default="1">
			<option value="1">Office</option>
			<option value="2">Distance</option>
		</param>
		<filter name="CATEGORY_ID" type="sql" default="" label="CATEGORY_ID" key_field="ID" value_field="LIB_TITLE" query="select id, lib_title from lib_categories t where t.active=1 order by t. lib_title asc" select_label="Select Category" />
		<param name="GRAPHTYPE" type="graphtype" default="" label="GRAPHTYPE"  />
		<!--<param name="GRAPHGROUP" type="graphgroup" default="" label="GRAPHGROUP" />-->
		<param name="CHANGE_DATE" type="calendar" default="" label="CHANGE_DATE"  must="1"/>
		<param name="ID" type="hidden" default="" />
	</params>
</install>
