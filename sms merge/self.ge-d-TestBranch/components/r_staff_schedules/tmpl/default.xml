<?xml version="1.0" encoding="UTF-8"?>
  <!DOCTYPE install SYSTEM "http://web.magti.ge/pda/defines/xml/grid.dtd">
<install type="component" version="1.5.0">
	<filters class="page_filter_block" type="h">
		<filter name="org" type="org" default="" label="org"/>
		<filter name="org_place" type="unit" default="" label="unit"  elementid="org" />
		<filter name="staffschedule" type="text" default="" label="STAFF_SCHEDULE_UNIT" geokbd="1" />
		<filter name="start_date" type="calendar" default="now" label="Date" format="%Y-%m-%d" />
		<filter type="buttons" default="" submit_label="Filter" reset_label="Clear" show_reset= "1" />
	</filters>
	<params class="table table-bordered table-condensed table-hover table-striped exporttable" id="exportable">
		<param order="" type="number" default="" label="#" rclass="text-center" />
		<param order="" type="print" label="staff_schedule" key="TITLE" rclass="text-left" />
		<param order="" type="org" label="ORG" key="ORG" rclass="text-left" T="1"/>
		<param order="" type="unit" label="UNIT" key="ORG_PLACE" rclass="text-left" T="1"/>
		<param order="" type="print" label="QUANTITY" key="QUANTITY" rclass="text-center" />
		<param order="" type="print" label="assigned_worker_quantity" key="S_COUNT" rclass="text-center" />
		<param order="" type="numberdiff" label="plus" n1="S_COUNT" n2="QUANTITY" polus="1" color="red" rclass="text-center" />
		<param order="" type="numberdiff" label="minus" n2="S_COUNT" n1="QUANTITY" polus="1" color="red" rclass="text-center" />

		<param order="" type="print" label="SALARY" key="SALARY" rclass="text-center" />
		<param order="" type="print" label="SUMSALARY" key="SUMSALARY" rclass="text-center" />
		<param order="" type="print" label="REAL_SALARY" key="REAL_SALARY" rclass="text-center" />
		<param order="" type="print" label="MID_SALARY" key="MID_SALARY" rclass="text-center" />
		<param order="" type="numberdiff" label="SALARY_PLUS" n1="REAL_SALARY" n2="SUMSALARY" polus="1" color="red" rclass="text-center" />
		<param order="" type="numberdiff" label="SALARY_MINUS" n1="SUMSALARY" n2="REAL_SALARY" polus="1" color="red" rclass="text-center" />

		<param order="" type="workerslink" label="show_workers" key="ID" rclass="text-center" hclass="text-center" />
	</params>
</install>
