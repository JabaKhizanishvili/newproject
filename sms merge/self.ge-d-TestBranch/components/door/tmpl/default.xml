<?xml version="1.0" encoding="UTF-8"?>
<install type="component" version="1.5.0">
	<params>
		<param name="LIB_TITLE" type="text" default="" label="Title" must="1"  class="kbd" />
		<param name="LIB_DESC" type="text" default="" label="Description"  class="kbd"  />
		<param name="ACTIVE" type="radio" default="1">
			<option value="1">Active Status</option>
			<option value="0">Inactive</option>
		</param>
	</params>
	<params group="door">
		<param name="OFFICE" type="office" default="" label="OFFICE" T="1" />
		<param name="CODE" type="text" default="" label="CODE"   />
		<param name="TYPE" type="list" default="1">
			<option value="1">In Door</option>
			<option value="2">Out Door</option>
			<option value="3">In/Out Door</option>
			<option value="5">Additional Door In</option>
			<option value="6">Additional Door Out</option>
		</param>
		<param name="DEFDOOR" type="radio" default="0" label="DEFDOOR">
			<option value="1">Yes</option>
			<option value="0">No</option>
		</param>
		<param name="ID" type="hidden" default="" />
	</params>
</install>
