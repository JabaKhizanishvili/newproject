<?xml version="1.0" encoding="UTF-8"?>
<install type="component" version="1.5.0">
	<params>
		<param name="WORKER" type="slfstaff" case="8" default="" label="Worker" must="1" />
		<param name="STATUS" type="radio" default="1" >
			<option value="1">B_open</option>
			<option value="2">BL_CLOSE</option>
			<option value="3">BL_CONFIRMED</option>
		</param>
		<param name="START_DATE" type="calendar" default="now" label="Bulletins Start" must="1" />
		<param name="END_DATE" type="calendar" default="now + 40 day" label="Bulletins End" />
		<param name="DAY_COUNT" type="text" default="0" label="DAY COUNT" />
		<param name="UCOMMENT" type="textarea" default="" label="UCOMMENT" geokbd="1"/>
		<param name="FILES" type="files" default="" label="FILES" />

		<param name="ID" type="hidden" default="" />
		<param name="APPROVE" type="hidden" default="0" />
		<param name="SYNC" type="hidden" default="0" />
		<param name="DEL_USER" type="hidden" default="0" />
	</params>
</install>
