<?xml version="1.0" encoding="UTF-8"?>
<install type="component" version="1.5.0">
	<params>
		<param name="LIB_TITLE" type="text" default="" label="Title" must="1"  geokbd="1" />
		<param name="LIB_DESC" type="textarea" default="" label="Description" geokbd="1" />
		<group>
			<param name="ACTIVE" type="radio" default="1">
				<option value="1">Active Status</option>
				<option value="0">Inactive</option>
			</param>
			<param name="ORDERING" type="text" default="999" label="ORDERING"  />
		</group>
		<param name="ID" type="hidden" default="" />
	</params>
	<params group="details">
		<param name="PAYMENT_TYPE" type="list" default="-1" label="PAYMENT_TYPE" must="1">
			<option value="-1">select category</option>
			<option value="1">Fixed_Payment</option>
			<!--<option value="2">Hour_Payment</option>-->
		</param>
		<param name="TAXES_YN" type="radio" default="" must="1" description="taxWarning">
			<option value="1">IncomeTax</option>
			<option value="2">PensionTax</option>
			<option value="0">NoTax</option>
		</param>
		<param name="ACCURACY_PERIOD" type="accuracyperiods" default="-1" label="ACCURACY_PERIOD" must="1"/>
	</params>
</install>
