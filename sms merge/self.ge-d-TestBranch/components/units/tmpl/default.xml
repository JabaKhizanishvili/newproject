<?xml version="1.0" encoding="UTF-8"?>
  <!DOCTYPE install SYSTEM "http://web.magti.ge/pda/defines/xml/grid.dtd">
<install type="component" version="1.5.0">
	<filters class="page_filter_block" type="h">
		<filter name="lib_title" type="text" default="" label="Title" geokbd="1" />
		<filter name="org" type="org" default="" label="org" />
		<filter name="ulevel" type="level" default="" label="level" />
		<filter name="type" type="utype" default="" label="type" />
		<filter name="active" type="state" default="-1" label="State">
			<option value="-1">Select State</option>
			<option value="0">Inactive</option>
			<option value="1">Active Status</option>
		</filter>
		<filter name="org_place" type="unit" default="0" admin="" label="org_place"  elementid="org" />
		<filter name="unit_code" type="text" default="" label="UNIT_CODE" />
		<filter type="buttons" default="" submit_label="Filter" reset_label="Clear" show_reset= "1" />
	</filters>
	<params class="table table-bordered table-condensed table-hover table-striped exporttable" id="exportable">
		<param order="" type="number" default="" label="#" rclass="text-center" />
		<param order="" type="checkall" default="" label="" rclass="text-center"   />
		<param order="" type="editlink" label="Title" key="TITLE" rclass="text-left" />
		<param order="" type="print" label="Description" key="LIB_DESC" rclass="text-left" />
		<param order="" type="org" label="org" key="ORG" rclass="text-left" T="1"/>
		<param order="" type="unittype" label="Type" key="TYPE" rclass="text-left" T="1"/>
		<param order="" type="print" label="ulevel" key="ULEVEL" rclass="text-left" />
		<!--<param order="t.lft" type="print" label="LFT" key="LFT" rclass="text-left" />-->
		<!--<param order="t.rgt" type="print" label="RGT" key="RGT" rclass="text-left" />-->
		<param order="" type="print" label="UNIT_CODE" key="UNIT_CODE" rclass="text-left" />
		<param order="" type="print" label="priority" key="ORDERING" rclass="text-center" />
		<param order="t.active" type="state" label="State" rclass="text-center" state_id='ID' state_key="ACTIVE" />
		<!--<param order="t.id" type="print" label="ID" key="ID" rclass="text-center" />-->
	</params>
</install>