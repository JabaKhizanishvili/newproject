<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE table SYSTEM "http://web.magti.ge/pda/defines/xml/table.dtd">
<table name="system_sessions">
	<column name="id" type="number" null="n" />
	<column name="session_id" type="varchar2" length="400" />
	<column name="session_data" type="varchar2" length="4000" />
	<column name="session_type" type="number" />
	<column name="username" type="varchar2" length="400" />
	<column name="start_date" type="date" />
	<column name="end_date" type="date" />
	<column name="log_ip" type="varchar2" length="50" />
	<column name="browser_name" type="varchar2" length="4000" />
	<column name="os_name" type="varchar2" length="4000" />
	<column name="log_user_id" type="number" />
	<column name="log_date" type="date" />
	<column name="client_id" type="number" default="00" />
	<column name="session_token" type="varchar2" length="400"  />
</table>