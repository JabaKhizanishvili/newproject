		<column name="pension" type="number" default="00" />
		<column name="accrual_type" type="number" default="00" />
		<column name="periodicity" type="number" default="00" />
		<column name="start_date" type="date" />
		<column name="end_date" type="date" />
		<column name="benefit" type="number" default="00" />
		<column name="type" type="number" default="00" />
		<column name="share_type" type="number" default="00" />
		<column name="calculation_day" type="number" />
		<column name="modify_user" type="number" />
		<column name="modify_date" type="date" />
		<column name="gross_tax_type" type="number" />
		<column name="accrual_set" type="number" />

		<column name="c_worker_share" type="number" />
		<column name="c_company_share" type="number" />
		<column name="accrued_income" type="number" />
		<column name="accrued_pension" type="number" />
	</columns>
	<sequence  name="sqs_f_benefit_types" />
</table>
