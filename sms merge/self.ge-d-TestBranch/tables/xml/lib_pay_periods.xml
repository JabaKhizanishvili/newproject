<!DOCTYPE table SYSTEM "http://web.magti.ge/pda/defines/xml/table.dtd">
<table name="slf_pay_periods" history="yes">
	<columns>
		<column name="id" type="number" null="n" />
		<column name="pid" type="number"/>
		<column name="p_type" type="number"/>
		<column name="p_code" type="varchar2"  length="500"/>
		<column name="p_start" type="date"/>
		<column name="p_end" type="date"/>
		<column name="status" type="number" default="00"/>
	</columns>
	<sequence  name="sqs_pay_periods" />
</table>