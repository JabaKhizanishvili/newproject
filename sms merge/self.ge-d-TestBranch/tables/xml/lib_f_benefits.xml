<!DOCTYPE table SYSTEM "http://web.magti.ge/pda/defines/xml/table.dtd">
<table name="lib_f_benefits" history="yes">
	<columns>
		<column name="id" type="number" null="n" />
		<column name="lib_title" type="varchar2" length="500" Ti="1" />
		<column name="lib_desc" type="varchar2" length="4000" />
		<column name="active" type="number" null="n" default="1 " />
		<column name="ordering" type="number" null="n" default="999" />
		<column name="client_id" type="number" default="00" />
		
		<column name="accounting_code" type="varchar2" length="500" />
		<column name="fields" type="varchar2" length="500" />
		<column name="modify_user" type="number" />
		<column name="modify_date" type="date" />

		<column name="IDENTIFIER" type="number" />
		<column name="BENEFIT_END_DATE" type="number" />
	</columns>
	<sequence  name="sqs_f_benefits" />
</table>
